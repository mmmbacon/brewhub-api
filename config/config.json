{
  "development": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgresql"
  },
  "test": {
    "use_env_variable": "DATABASE_TEST_URL",
    "dialect": "postgresql"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgresql",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
